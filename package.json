{
  "name": "obfuscator",
  "version": "1.0.0",
  "bin": "./bin/obfuscate.js",
  "dependencies": {
    "@babel/core": "^7.28.0",
    "@babel/generator": "^7.28.0",
    "@babel/parser": "^7.28.0",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/traverse": "^7.28.0",
    "@babel/types": "^7.28.0",
    "buffer": "^6.0.3",
    "cheerio": "^1.1.0",
    "commander": "^14.0.0",
    "css": "^3.0.0",
    "cssnano": "^7.0.7",
    "fs-extra": "^11.3.0",
    "javascript-obfuscator": "^4.1.1",
    "node-html-parser": "^7.0.1",
    "postcss": "^8.5.6",
    "postcss-value-parser": "^4.2.0",
    "shorthash": "^0.0.2",
    "shuffle-array": "^1.0.1"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "scripts": {
    "start": "node ./bin/obfuscate.js",
    "build": "pnpm run build:linux && pnpm run build:macos-x64 && pnpm run build:macos-arm64 && pnpm run build:win",
    "build:linux": "pkg . --out-path dist --target node18-linux-x64 --output dist/obfuscate-linux-x64",
    "build:macos-x64": "pkg . --out-path dist --target node18-macos-x64 --output dist/obfuscate-macos-x64",
    "build:macos-arm64": "pkg . --out-path dist --target node18-macos-arm64 --output dist/obfuscate-macos-arm64",
    "build:win": "pkg . --out-path dist --target node18-win-x64 --output dist/obfuscate-win-x64.exe",
    "clean": "rm -rf dist/"
  },
  "pkg": {
    "assets": [
      "node_modules/@babel/preset-typescript/**/*",
      "node_modules/@babel/plugin-transform-typescript/**/*",
      "node_modules/cssnano/dist/**/*",
      "node_modules/postcss/lib/**/*",
      "node_modules/javascript-obfuscator/dist/**/*",
      "node_modules/cheerio/lib/**/*",
      "node_modules/node-html-parser/dist/**/*"
    ],
    "scripts": [
      "bin/**/*.js"
    ]
  }
}
